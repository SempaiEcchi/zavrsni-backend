{
  "name": "firmusbackendnest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "yarn": "1.22.19"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "build": "NODE_ENV=prod nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=dev nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "NODE_ENV=dev nest start --debug --watch",
    "start:prod": "NODE_ENV=prod ts-node dist/main.js",
    "typeorm": "ts-node  ./node_modules/typeorm/cli.js",
    "migration:run": "yarn run typeorm-ts-node-esm migration:run -- -d ./src/typeOrm.config.ts",
    "migration:generate": "npm run typeorm -- -d ./src/typeOrm.config.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- c ./src/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- -d ./src/typeOrm.config.ts migration:revert",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@adminjs/design-system": "latest",
    "@adminjs/express": "^6.1.0",
    "@adminjs/fastify": "^4.1.2",
    "@adminjs/nestjs": "^6.1.0",
    "@adminjs/typeorm": "^5.0.1",
    "@adminjs/upload": "^4.0.2",
    "@aws-sdk/client-s3": "^3.540.0",
    "@faker-js/faker": "^8.4.1",
    "@fastify/multipart": "^8.2.0",
    "@fastify/static": "^7.0.1",
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "@googlemaps/places": "^1.3.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.5",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.3.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.5",
    "@nestjs/platform-fastify": "^10.3.5",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@types/geojson": "latest",
    "adminjs": "^7.7.4",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "express-basic-auth": "^1.2.1",
    "express-formidable": "1.2.0",
    "express-session": "1.18.0",
    "fastify-multipart": "^5.4.0",
    "firebase-admin": "^12.0.0",
    "ip-fetch": "^2.0.0",
    "jwks-rsa": "^3.1.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.29.2",
    "passport": "latest",
    "passport-jwt": "latest",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.4.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.5",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.11",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/multer": "latest",
    "@types/node": "20.11.30",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@4.0.2"
}
